+def seconds_to_ui_time(seconds):
+    if seconds >= 60 * 60 * 24:
+        return "%d day(s) %d:%02d:%02d" % (seconds / (60 * 60 * 24),
+                                           (seconds / (60 * 60)) % 24,
+                                           (seconds / 60) % 60,
+                                           seconds % 60)
+    if seconds >= 60 * 60:
+        return "%d:%02d:%02d" % (seconds / (60 * 60), (seconds / 60) % 60,
+                                 (seconds % 60))
